<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="style.css"  rel="stylesheet" type="text/css"> 
    <script src="mazat.js"></script>
    <title>admin</title>
</head>
<body>
  <a href="/newuser">Registration</a>
    <table>
      <body>
        <table>
          <thead>
            <tr>
              <th>ID</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Age</th>
              <th>City</th>
            </tr>
          </thead>
          <tbody>
            <% result.forEach(function(item) { %>
              <tr>
                <td><%= item.id %></td>
                <td><%= item.fname %></td>
                <td><%= item.lname %></td>
                <td><%= item.age %></td>
                <td><%= item.city %></td> 
                <td><button class="smazatB" onclick="mazat(<%=item.id%>)">smazat</button></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </body>
      </html>