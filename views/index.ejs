<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="style.css"  rel="stylesheet" type="text/css"> 
    <script src="script.js"></script>
    <title>admin</title>
</head>
<body>
  <a href="/newuser">Registration</a>
    <table>
      <body>
        <table>
          <thead>
            <tr>
              <th>ID</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Age</th>
              <th>City</th>
            </tr>
          </thead>
          <tbody>
            <% result.forEach(function(item) { %>
              <tr>
                <td><%=item.id%></td>
                <td><input class="inputTD" id="<%=item.id%>.fname" type="text" value="<%=item.fname%>"></td>
                <td><input class="inputTD" id="<%=item.id%>.lname" type="text" value="<%=item.lname%>"></td>
                <td><input class="inputTD" id="<%=item.id%>.age" type="text" value="<%=item.age%>"></td>
                <td><input class="inputTD" id="<%=item.id%>.city" type="text" value="<%=item.city%>"></td>

                <td><button class="smazatB" onclick="mazat(<%=item.id%>)">smazat</button></td>
                <td><button class="smazatB" onclick="ulozto(<%=item.id%>)">upravit</button></td>
              </tr>
            <% }); %>
          </tbody>
        </table>
      </body>
      </html>